{
  "author": "Aziz Baatout",
  "name": "user-service",
  "description": "User microservice",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "prisma": {
    "schema": "src/data/schema.prisma",
    "seed": "tsx ./src/data/seed.ts"
  },
  "scripts": {
    "start:dev": "tsx watch --env-file .env src/index.ts",
    "start": "node dist/src/index.js",
    "build": "tsc",
    "lint": "eslint . --fix",
    "migrate:dev": "prisma db push",
    "seed": "prisma db seed",
    "test": "env-cmd -f .env.test jest --runInBand --silent",
    "test:coverage": "env-cmd -f .env.test jest --runInBand --coverage --silent"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "argon2": "^0.41.1",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "prisma": "^6.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.12.1",
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/geoip-lite": "^1.4.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "env-cmd": "^10.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-jest": "^28.10.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
